{
  "name": "linter-php",
  "main": "./lib/main",
  "version": "0.0.15",
  "description": "Lint PHP on the fly, using php -l",
  "repository": "https://github.com/AtomLinter/linter-php",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "coffeelintConfig": {
    "max_line_length" : {
      "level" : "error",
      "value" : 1000
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {},
  "_atomModuleCache": {
    "version": 1,
    "dependencies": [],
    "extensions": {
      ".coffee": [
        "lib/helpers.coffee",
        "lib/main.coffee"
      ],
      ".json": [
        "package.json"
      ]
    },
    "folders": []
  }
}
