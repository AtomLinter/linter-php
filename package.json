{
  "name": "linter-php",
  "main": "./lib/main",
  "version": "1.1.1",
  "description": "Lint PHP on the fly, using php -l",
  "repository": "https://github.com/AtomLinter/linter-php",
  "license": "MIT",
  "activationHooks": [
    "language-php:grammar-used"
  ],
  "dependencies": {
    "atom-linter": "^3.3.1",
    "atom-package-deps": "^3.0.0"
  },
  "devDependencies": {
    "coffeelint": "^1.13.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
